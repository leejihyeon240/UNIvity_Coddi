<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Register2Activity"
        android:background="@drawable/login_bg">

    <ImageView android:layout_width="338.29dp"
               android:layout_height="88.29dp"
               android:adjustViewBounds="true"
               app:layout_constraintLeft_toLeftOf="parent"
               app:layout_constraintRight_toRightOf="parent"
               android:src="@drawable/login_logo"
               android:id="@+id/imageView8"
               app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="70.85dp"/>
    <TextView
            android:text="회원가입"
            android:fontFamily="@font/noto_sans_bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView11"
            android:textSize="25sp" android:textStyle="bold" android:textColor="#FFFFFF"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="200.85dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="38.85dp"/>
    <TextView
            android:text="개인 정보를 입력해주세요."
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="11sp"
            android:id="@+id/textView3"
            android:fontFamily="@font/noto_sans"
            android:textColor="#FFFFFF"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="244dp" android:layout_marginLeft="38.85dp"/>
    <TextView
            android:text="이름"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/noto_sans_bold"
            android:id="@+id/textView4" android:textSize="16sp"
            android:textColor="#FFFFFF"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="45.71dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="317.71dp"/>
    <TextView
            android:text="닉네임"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView6"
            android:fontFamily="@font/noto_sans_bold"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="317.71dp"
            android:layout_marginLeft="222.85dp"
            android:textColor="#FFFFFF"/>
    <TextView
            android:text="전화번호"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView12"
            android:fontFamily="@font/noto_sans_bold"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="45.71dp" android:layout_marginTop="418.57dp"
            android:textColor="#FFFFFF"/>
    <TextView
            android:text="이메일 인증"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView7"
            android:textSize="16sp"
            android:textColor="#FFFFFF"
            android:textStyle="bold"
            android:fontFamily="@font/noto_sans_bold"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="519.71dp" android:layout_marginLeft="45.71dp"/>
    <TextView
            android:text="*인증메일이 발송되었습니다."
            android:fontFamily="@font/noto_sans"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvPwdChk"
            android:visibility="visible"
            android:textColor="#FFF27E"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="54.85dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="599.42dp"/>
    <ImageView
            android:layout_width="333.71dp"
            android:layout_height="51.08dp"
            android:adjustViewBounds="true"
            app:srcCompat="@drawable/next" android:id="@+id/nextBtn2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="678dp"/>
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayout"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="345.71dp"
            android:layout_marginLeft="38.85dp">
        <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="20dp">

            <ImageView
                    android:layout_width="156.57dp"
                    android:layout_height="42.29dp"
                    android:adjustViewBounds="true"
                    app:srcCompat="@drawable/small_box"
                    android:id="@+id/imageView"
            />
            <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:inputType="textPersonName"
                    android:ems="10"
                    android:hint="Ex.김유니"
                    android:privateImeOptions="defaultInputmode=korean"
                    android:textSize="12sp"
                    android:singleLine="false" android:backgroundTint="#00000000"
                    android:layout_marginStart="10dp"
                    android:id="@+id/editFindName"
                    android:layout_gravity="left|center"/>
        </FrameLayout>
    </LinearLayout>
    <LinearLayout
            android:orientation="vertical"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="345.71dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="38.85dp">
        <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:layout_marginBottom="20dp">

            <ImageView
                    android:layout_width="156.57dp"
                    android:layout_height="42.29dp"
                    android:adjustViewBounds="true"
                    app:srcCompat="@drawable/small_box"
            />
            <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:inputType="textPersonName"
                    android:ems="10"
                    android:hint="Ex.유니사랑"
                    android:privateImeOptions="defaultInputmode=korean"
                    android:textSize="12sp"
                    android:singleLine="false" android:backgroundTint="#00000000"
                    android:layout_marginStart="10dp"
                    android:id="@+id/editNickname"
                    android:layout_gravity="left|center"/>
        </FrameLayout>
    </LinearLayout>
    <LinearLayout
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="447.14dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
        <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent" android:layout_marginBottom="20dp">

            <ImageView
                    android:layout_width="333.71dp"
                    android:layout_height="42.29dp"
                    android:adjustViewBounds="true"
                    app:srcCompat="@drawable/insert"
            />
            <EditText
                    android:layout_width="284dp"
                    android:layout_height="wrap_content"
                    android:inputType="phone"
                    android:ems="10"
                    android:hint="Ex. 010-0000-0000"
                    android:textSize="12sp"
                    android:singleLine="false" android:backgroundTint="#00000000"
                    android:layout_marginStart="10dp"
                    android:id="@+id/editFindNumber"
                    android:layout_gravity="left|center"/>
        </FrameLayout>
    </LinearLayout>
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="548.28dp" app:layout_constraintEnd_toEndOf="parent">
        <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            <ImageView
                    android:layout_width="333.71dp"
                    android:layout_height="42.29dp"
                    app:srcCompat="@drawable/insert"
                    android:adjustViewBounds="true"
            />

            <EditText
                    android:layout_width="284dp"
                    android:layout_height="wrap_content"
                    android:inputType="phone"
                    android:ems="10"
                    android:textSize="12sp"
                    android:singleLine="false" android:backgroundTint="#00000000"
                    android:layout_marginStart="10dp"
                    android:id="@+id/editEmail"
                    android:layout_gravity="left|center"/>
            <TextView
                    android:text="인증하기"
                    android:textColor="#6D6890"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/noto_sans_bold"
                    android:id="@+id/tvEmailChk"
                    android:layout_gravity="center|right"
                    android:layout_marginRight="15dp"
                    android:textStyle="bold"/>
        </FrameLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>